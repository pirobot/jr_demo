<?xml version="1.0"?>

<robot name="jackrabbot" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:include filename="$(find jr_description)/urdf/primesense.urdf.xacro" />
  
  <gazebo> 
     <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so"> 
        <robotNamespace>/</robotNamespace>
     </plugin> 
  </gazebo>

  <link
    name="base_link">
    <inertial>
      <origin
        xyz="-0.21711 -0.0002007 0.84568"
        rpy="0 0 0" />
      <mass
        value="1.752" />
      <inertia
        ixx="0.0051007"
        ixy="1.2402E-05"
        ixz="-0.00021495"
        iyy="0.0059117"
        iyz="-6.6044E-06"
        izz="0.0043871" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://jr_description/meshes/base_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0 0.6 0 1" />
      </material>
    </visual>
    <collision>
      
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <box size="0.78 .65 .98"/>
      </geometry>
    </collision>
  </link>

  <link name="microstrain_imu_frame"/>
  
  <joint name="microstrain_imu_frame_joint" type="fixed">
    <origin xyz="-0.18877 0.0 0.74640" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="microstrain_imu_frame" />
  </joint>
  
  <link name="gps_rcvr_frame"/>
  
  <joint name="microstrain_gps_frame_joint" type="fixed">
    <origin xyz="-0.25728 0.0 0.94843" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="gps_rcvr_frame" />
  </joint>

  <link name="sick_frame" />
  <joint name="sick_frame_joint" type="fixed">
    <origin xyz="0.29345 0 0.26796" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="sick_frame" />
  </joint>
      
  <link name="rear_right_camera_frame">
    <inertial>
      <mass value="0.001"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>
  
  <joint name="rear_right_camera_frame_joint" type="fixed">
    <origin xyz="-0.28657 -0.050243 0.66008" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="rear_right_camera_frame" />
  </joint>

  <link name="rear_right_camera_optical_frame" />
  <joint name="rear_right_camera_optical_frame_joint" type="fixed">
    <origin xyz="0 0 0" rpy="-1.5708 0 2.1817" />
    <parent link="rear_right_camera_frame" />
    <child link="rear_right_camera_optical_frame" />
  </joint>
  
  <link name="rear_left_camera_frame">
    <inertial>
      <mass value="0.001"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
