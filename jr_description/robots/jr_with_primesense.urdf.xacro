    <parent link="motor_visual" />
    <child link="hokuyo_visual_link" />
    <axis xyz="1 0 0" />
  </joint>

  <link
    name="hokuyo_visual_link">
    <inertial>
      <origin
        xyz="0.042795 -0.0004254 -0.0031482"
        rpy="0 0 0" />
      <mass
        value="0.29823" />
      <inertia
        ixx="0.00027212"
        ixy="4.6535E-06"
        ixz="1.9506E-06"
        iyy="0.0003245"
        iyz="-3.2249E-06"
        izz="0.00021294" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
